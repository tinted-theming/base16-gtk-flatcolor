selected_fg_color:#bcbabe
tooltip_bg_color:#2e2a31
tooltip_fg_color:#bcbabe
titlebar_bg_color:#2e2a31
titlebar_fg_color:#bcbabe
menu_bg_color:#2e2a31
menu_fg_color:#bcbabe
