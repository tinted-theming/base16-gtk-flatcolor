selected_fg_color:#5a496e
tooltip_bg_color:#feffff
tooltip_fg_color:#5a496e
titlebar_bg_color:#feffff
titlebar_fg_color:#5a496e
menu_bg_color:#feffff
menu_fg_color:#5a496e
